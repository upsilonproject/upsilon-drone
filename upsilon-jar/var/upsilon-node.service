[Unit]
Description=upsilon-node
After=syslog.target network.target remote-fs.target nss-lookup.target

[Service]
User=upsilon
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=upsilon-node
ExecStart=/usr/bin/java -Djava.net.preferIPv4Stack=true -jar /usr/share/upsilon-node/upsilon-node.jar $OPTIONS

[Install]  
WantedBy=multi-user.target
